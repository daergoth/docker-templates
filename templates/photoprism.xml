<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Name>PhotoPrism</Name>
  <Description>PhotoPrism</Description>
  <Overview>PhotoPrism: Browse Your Life in Pictures</Overview>
  <Registry>https://hub.docker.com/r/photoprism/photoprism/</Registry>
  <Repository>photoprism/photoprism:latest</Repository>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>PHOTOPRISM_ADMIN_PASSWORD</Name>
      <Value>photoprism</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_DEBUG</Name>
      <Value>false</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_PUBLIC</Name>
      <Value>false</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_READONLY</Name>
      <Value>false</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_UPLOAD_NSFW</Name>
      <Value>true</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_DETECT_NSFW</Name>
      <Value>false</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_EXPERIMENTAL</Name>
      <Value>false</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_SITE_URL</Name>
      <Value>http://localhost:2342/</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_SITE_TITLE</Name>
      <Value>PhotoPrism</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_SITE_CAPTION</Name>
      <Value>Browse Your Life</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_SITE_DESCRIPTION</Name>
      <Value></Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_SITE_AUTHOR</Name>
      <Value></Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_HTTP_HOST</Name>
      <Value>0.0.0.0</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_HTTP_PORT</Name>
      <Value>2342</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_SETTINGS_HIDDEN</Name>
      <Value>false</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_DATABASE_DRIVER</Name>
      <Value>sqlite</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_THUMB_FILTER</Name>
      <Value>lanczos</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_THUMB_UNCACHED</Name>
      <Value>false</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_THUMB_SIZE</Name>
      <Value>2048</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_THUMB_LIMIT</Name>
      <Value>3840</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_JPEG_QUALITY</Name>
      <Value>90</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PHOTOPRISM_STORAGE_PATH</Name>
      <Value>/photoprism/storage</Value>
      <Mode/>
    </Variable>
  </Environment>
  <Network>bridge</Network>
  <Project>https://docs.photoprism.org/</Project>
  <TemplateURL>https://raw.githubusercontent.com/daergoth/docker-templates/master/templates/photoprism.xml</TemplateURL>
  <Icon>https://github.com/photoprism/photoprism/blob/develop/assets/static/img/favicon-512.png</Icon>
  <Config 
       Type="Path" 
       Name="Originals directory" 
       Target="/photoprism/originals" 
       Default="/mnt/user/appdata/photoprism/originals" 
       Mode="rw" 
       Description="This is where PhotoPrism will store its originals."
       Required="true">/mnt/user/appdata/photoprism/originals</Config>
  <Config 
       Type="Path" 
       Name="Import directory" 
       Target="/photoprism/import" 
       Default="/mnt/user/appdata/photoprism/import" 
       Mode="rw" 
       Description="This is where PhotoPrism will import new media from."
       Required="true">/mnt/user/appdata/photoprism/import</Config>
  <Config 
       Type="Path" 
       Name="Database directory" 
       Target="/photoprism/storage" 
       Default="/mnt/user/appdata/photoprism/storage" 
       Mode="rw" 
       Description="This is where PhotoPrism will store its database, cache and settings."
       Required="true">/mnt/user/appdata/photoprism/storage</Config>
  <Config 
       Type="Port" 
       Name="Web UI" 
       Target="2342" 
       Default="2342" 
       Mode="tcp" 
       Description="This is the Web UI Port, and is used to access PhotoPrism." 
       Display="always" 
       Required="true">2342</Config>
  <Config 
       Name="WebUI password" 
       Target="PHOTOPRISM_ADMIN_PASSWORD" 
       Default="photoprism" 
       Mode="" 
       Description="Container Variable: PHOTOPRISM_ADMIN_PASSWORD" 
       Type="Variable" 
       Display="always" 
       Required="true" 
       Mask="false">photoprism</Config>
  <Config 
       Name="Canonical / public site URL" 
       Target="PHOTOPRISM_SITE_URL" 
       Default="http://localhost:2342/" 
       Mode="" 
       Description="Container Variable: PHOTOPRISM_SITE_URL" 
       Type="Variable" 
       Display="always" 
       Required="true" 
       Mask="false">http://localhost:2342/</Config>
  <WebUI>http://[IP]:[PORT:2342]/</WebUI>
  <ExtraParams/>
</Container>